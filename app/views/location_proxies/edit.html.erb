<h1>Editing Location Proxy <%= @location_proxy.id %></h1>

<%= render Admin::ArticleComponent.new do |c| %>
  <% c.section do %>
    <%= render 'form', location_proxy: @location_proxy, location_group: @location_group %>
  <% end %>
  <% c.footer do %>
    <%= link_to 'Show', @location_proxy %>
    <%= link_to 'Back', [@location_group, :location_proxies] %>
  <% end %>
<% end %>
