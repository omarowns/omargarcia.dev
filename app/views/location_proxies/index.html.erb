<p id="notice"><%= notice %></p>

<h1>Location Proxies</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>Parent</th>
      <th>Parent Type</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_proxies.each do |location_proxy| %>
      <tr>
        <td><%= location_proxy.position %></td>
        <td><%= location_proxy.parent_id %></td>
        <td><%= location_proxy.parent_type %></td>
        <td><%= location_proxy.location_id %></td>
        <td><%= link_to 'Show', location_proxy %></td>
        <td><%= link_to 'Edit', [:edit, location_proxy] %></td>
        <td><%= link_to 'Destroy', location_proxy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location Proxy', [:new, :location_group, :location_proxy] %>
