<h1>Editing Interest Group <%= @interest_group.id %></h1>

<%= render Admin::ArticleComponent.new do |c| %>
  <% c.section do %>
    <%= render 'form', interest_group: @interest_group %>
  <% end %>
  <% c.footer do %>
    <%= link_to 'Show', @interest_group %>
    <%= link_to 'Back', interest_groups_path %>
  <% end %>
<% end %>
