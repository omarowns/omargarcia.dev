<h1>Editing Work Group <%= @work_group.id %></h1>

<%= render Admin::ArticleComponent.new do |c| %>
  <% c.section do %>
    <%= render 'form', work_group: @work_group %>
  <% end %>
  <% c.footer do %>
    <%= link_to 'Show', @work_group %>
    <%= link_to 'Back', :work_groups %>
  <% end %>
<% end %>
