<p id="notice"><%= notice %></p>
<h1>Location Group <%= @location_group.id %></h1>
<%= render Admin::ArticleComponent.new do |c| %>
  <% c.section do %>
    <p>
      <strong>Title:</strong>
      <%= @location_group.title %>
    </p>

    <p>
      <strong>Profile:</strong>
      <%= @location_group.profile_id %>
    </p>
  <% end %>
  <% c.footer do %>
    <%= link_to 'Edit', edit_location_group_path(@location_group) %>
    <%= link_to 'Back', location_groups_path %>
  <% end %>
<% end %>

<%= render Admin::HasMany::LocationsComponent.new(model: @location_group) %>
<%= render Admin::HasMany::ImagesComponent.new(model: @interest_group) %>
